        <name>UserCreationRequest</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
    // Get data from the form submission
    var username = request.body.data.username;  // e.g., form field: username
    var displayName = request.body.data.display_name;  // form field: display_name
    var email = request.body.data.email;  // form field: email

    // Build the SCIM payload
    var userData = {
        schemas: ["urn:ietf:params:scim:schemas:core:2.0:User"],
        userName: username,
        displayName: displayName,
        active: true,
        emails: [
            {
                value: email,
                type: "work",
                primary: true
            }
        ]
    };

    // Set up HTTP client and send request
    var client = new GlideHTTPClient();
    var url = 'https://earth-apis.my.evidian.com/scim/v2/api/Users';
    var requestObj = new GlideHTTPRequest('POST', url);
    
    // Add the Authorization header with the token
    var token = 'eyJraWQiOiJyc2EyNTZfc2lnIiwiYWxnIjoiUlMyNTYifQ..pt39E49E7BqSKxKQ7v-bblrJHsNHxIbZJ0QMl_QqOqW5pcC47z-WH4dvUTzTnqiMQk7C7fhcAcqWp9XU26UIHHPPuxzDuUQiaszqrfjOTR6gL7PHcEqBuUaSjK8Z3bc7pYaJ1v_yuKczsUXn-it2TRsMZ-LhR_8K2oG4Y4u1kmygfF5dR8iFZbvIZ1eoD_O859uOrRGYCcixW1YlWzLV5_czZZheuCDLmsoiJAICppbE5qcObamqFUKSyQhYuJQLISp-7Sogyw21Nji3g43jGNJ9i1LovTOx3g0bQ7FPWvF1P9nixfMVFsUIF6vclAvZ5gTT_8a4Ck3GMNWn4o_mYQ';  // Replace with your actual token
    requestObj.addHeader('Authorization', 'Bearer ' + token);
    requestObj.addHeader('Content-Type', 'application/json');
    requestObj.setRequestBody(JSON.stringify(userData));

    var httpResponse = requestObj.execute();
    
    // Check the response status code
    if (httpResponse.getStatusCode() == 201) {
        // Successfully created user
        response.setStatus(201);
        response.setBody(httpResponse.getBody());
    } else {
        // Handle error
        response.setStatus(httpResponse.getStatusCode());
        response.setBody(httpResponse.getBody());
    }
})(request, response);
]]></operation_script>
