        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
    var restMessage = new sn_ws.RESTMessageV2('UserCreationOrbion'); // The name of your REST message
    //restMessage.setHttpHeader('Authorization', 'Bearer your_token_here'); // Add your token here
    restMessage.setHttpHeader('Content-Type', 'application/json'); // Ensure the content type is JSON

    // Manually create the request body
    var requestBody = {
        "schemas": ["urn:ietf:params:scim:schemas:core:2.0:User"],
        "userName": current.username,  // Use your ServiceNow record fields
        "displayName": current.display_name,
        "active": true,
        "emails": [
            {
                "value": current.email,  // Again, use fields from your ServiceNow record
                "type": "work",
                "primary": true
            }
        ]
    };

    // Convert the object to a JSON string
    restMessage.setRequestBody(JSON.stringify(requestBody));

    // Execute the request and handle the response
    try {
        var response = restMessage.execute();
        var status = response.getStatusCode();
        if (status == 200) {
            gs.info("User creation request sent successfully.");
        } else {
            gs.error("Failed to send user creation request. Status: " + status);
            gs.error("Response: " + response.getBody());
        }
    } catch (ex) {
        gs.error("Error sending user creation request: " + ex.message);
    }
})(current, previous);
]]></script>
