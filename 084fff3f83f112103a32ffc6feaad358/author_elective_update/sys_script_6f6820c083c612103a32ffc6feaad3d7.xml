        <script><![CDATA[if (current.operation() === 'insert') {
    // Create the RESTMessageV2 object for your REST Message
    var restMessage = new sn_ws.RESTMessageV2('UserCreationOrbion', 'POST');

    // Construct the JSON payload
    var requestBody = {
        schemas: ["urn:ietf:params:scim:schemas:core:2.0:User"],
        userName: current.user_name.toString(), // Get username from the form
        displayName: current.display_name.toString(), // Get display name from the form
        active: true, // Always set the user as active
        emails: [
            {
                value: current.email.toString(), // Get email from the form
                type: "work",
                primary: true
            }
        ]
    };

    // Set the Request Body
    restMessage.setRequestBody(JSON.stringify(requestBody));

    // Execute the REST Message
    var response;
    try {
        response = restMessage.execute();
        var responseBody = response.getBody();
        var httpStatus = response.getStatusCode();

        // Log the response
        gs.info("Response: " + responseBody + " | HTTP Status: " + httpStatus);
    } catch (ex) {
        // Handle errors
        gs.error("Error executing REST Message: " + ex.message);
    }
}]]></script>
